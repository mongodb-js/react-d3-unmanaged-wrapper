  },
  "peerDependencies": {
    "react": ">=0.14.x <=15.x",
    "react-dom": ">=0.14.x <=15.x"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-preset-react": "^6.11.1",
    "eslint": "^3.2.2",
    "eslint-config-mongodb-js": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "mocha": "^2.3.4",
